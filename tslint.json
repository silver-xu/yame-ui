{
    "extends": [
        "tslint:recommended",
        "tslint-react",
        "tslint-eslint-rules",
        "tslint-config-prettier"
    ],
    "linterOptions": { "exclude": ["node_modules/**/*.ts"] },
    "rules": {
        "quotemark": [true, "single", "jsx-double"],
        "no-console": [false],
        "interface-over-type-literal": false,
        "object-literal-sort-keys": false,
        "no-empty": false
    }
}
